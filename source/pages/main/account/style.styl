main-account {
  //width: 100%
  //height: 100%
  //margin-top: 25px;
  .bomb-used {
  //visibility hidden
    display: flex;
    justify-content center
    align-items center
    z-index: 100;
    background-color: rgba(0, 0, 0, 0.3);
    width: 100%;
    // height: 100%;
    position: absolute;
    left: 0;
    top:-25px;
    bottom:-25px;
    .bomb-content {
      z-index: 200
      width: 510px;
      height: 270px;
      border-radius 5px
      background-color: #ffffff;
      .bomb-top {
        margin-top: 15px
        position: relative;
        height: 50px
        line-height: 50px
        text-align: center
        .bomb-close {
          display: block
          position: absolute;
          top: 0;
          right: 20px;
          width: 20px
          height: 20px
          background: url("../../../static/images/close.png") right top no-repeat
        }

        .bomb-title {
          width: 130px
          margin 0 auto
          font-size: 16px;
          font-family: "PingFang SC";
          color: rgb(252, 71, 74);
          padding-left: 30px
          background url("../../../static/images/warnIcon.png") left center no-repeat
        }
      }
      table {
        width: 460px
        margin: 0 auto
        td {
          border: 1px solid #d3d8dc
          border-collapse: collapse
          vertical-align middle
          text-align center
          height: 45px
        }
        tr:nth-child(odd) {
          background: #ffffff
        }
        tr:nth-child(even) {
          background #f6f7fb
        }
      }

    }
  }
