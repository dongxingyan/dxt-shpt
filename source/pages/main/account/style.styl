  .screen {
  //height: 100%
    margin-top: 25px;
  //padding-bottom:20px;
    position relative
    flex: 1;
    background #ecf1f5
    .list {
    //height: 100%
      flex: 1
      background: f6f7fb;
      margin: 25px 20px 15px 25px
      display: flex
      flex-direction column
      justify-content space-between
      .tips {
        width: 580px
        height: 30px
        line-height: 30px
        padding-left: 45px
        margin-bottom 10px
        background #f1bec1
        font-size: 16px;
        font-family: "PingFang SC";
        color: rgb(252, 71, 74);
        position relative
      }
      .tips::before {
        content url("../../../static/images/warnIcon.png")
        display: block
        position absolute
        width: 20px
        height: 20px
        top: 4px
        left: 15px
      }
      .please {
        height: 45px
        line-height: 45px
        font-size: 18px;
        font-family: "PingFang SC";
        color: rgb(43, 50, 69);

      }
      .ticket {
        height: 48px
        margin-bottom: 20px
        display: flex
        position relative
        .ticket-number {
          height: 48px
          width: 300px
          padding-left 25px
          margin-right 15px
          border: 1px solid #d3d8dc
          border-radius 5px
          font-size: 18px;
        //position relative
          font-family: "PingFang SC";
          color: rgb(153, 158, 163);
          background-color #fff
        }
        .clear-value {
          width: 20px
          height: 20px
          background-color blue
          position: absolute
          top: 15px;
          left: 265px;
          background url("../../../static/images/delete.png")
        }

        .validate {
          cursor pointer
          width: 135px
          height: 48px
          line-height: 1
          border none
          background #23d1e3
          text-align center
          border-radius 5px
          font-size: 22px;
          font-family: "PingFang SC";
          color: rgb(255, 255, 255);
        }
      }
      .record {
        flex 1
        table {
          flex 1
          table, tr, td {
            padding: 0 5px;
            border: 1px solid #d3d8dc
            border-collapse: collapse
            vertical-align middle
            flex 1
            th {
              height: 36px
              line-height: 36px
              padding: 0 25px 0;
              text-align left
              border: 1px solid #d3d8dc
              background:#fff;
            }
            td {
              height: 45px
            }
          }
          tr:nth-child(even) {
            background: #ffffff
          }
          tr:nth-child(odd) {
            background #f6f7fb
          }
        }

      }
      .paging {
        height: 35px
        display flex
        justify-content flex-end
        .page-common {
          font-size: 12px
          display: block
          width: 35px
          height: 35px
          line-height: 35px
          text-align center
          border: 1px solid #aaaaaa
          margin-right: 10px;
          color: #393f51
          cursor pointer
          font-weight: 700

        }
        .bg{
          background-color:rgba(128,128,128,.3)
          border:none;
        }
        ul {
          padding-left: 0;
        }
        .pageNum {
          list-style none
        }
        .current {
          background: #23d1e3
          border: none
        }
      }
    }
  }
