main-order-manager {
  width: 100%
  height: 100%
  .main {
    width: 100%
    height: 100%
    display: flex
    flex-direction column

    .list {
    //height: 100%
      background: f6f7fb;
      margin: 30px 30px 20px
      display: flex
      flex-direction column
      justify-content space-between
      table {
      //height: 300px
      //overflow auto
        flex 1
      }
      table, tr, td, th {
        text-align center
        padding: 0 5px;
        border: 1px solid #d3d8dc
        border-collapse: collapse
        vertical-align middle
        flex 1
        th {
          height: 43px
        }
        td {
          height: 90px
        }
        tr:nth-child(odd) {
          background: #ffffff
        }
        tr:nth-child(even) {
          background #f6f7fb
        }
        .tl {
          text-align left
        }
      }
      .paging {
        height: 35px
        display flex
        justify-content flex-end
        .page-common {
          font-size: 12px
          display: block
          width: 35px
          height: 35px
          line-height: 35px
          text-align center
          border: 1px solid #aaaaaa
          margin-right: 10px;
          color: #393f51
          cursor pointer
          font-weight: 700
        }
        .bg{
          background-color:rgba(128,128,128,.3)
          border:none;
        }
        ul {
          padding-left: 0;
        }
        .pageNum {
          list-style none
        }
        .current {
          background: #23d1e3
          border: none
        }
      }
    }
  }
}